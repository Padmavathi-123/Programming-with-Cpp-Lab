#include <iostream> 
#include <vector> 
#include <list> 
using namespace std 
int main() { 
    // VECTOR demonstration 
    vector<int> v = {10, 20, 30};
    cout << "Vector elements: "; 
    for (int x : v) cout << x << " "; 
    cout << endl; 
    v.push_back(40); 
    v.erase(v.begin()); 
    cout << "After insertion and deletion: "; 
    for (int x : v) cout << x << " "; 
    cout << endl; 
    // LIST demonstration 
    list<int> l = {5, 10, 15}; 
    cout << "\nList elements: "; 
    for (int x : l) cout << x << " "; 
    cout << endl; 
    l.push_front(0); 
    l.push_back(20); 
    l.remove(10); 
    cout << "After insertion and deletion: "; 
    for (int x : l) cout << x << " "; 
    cout << endl; 
    return 0; 
} 
